#!/bin/sh -xv

# Import the ANT sources and binary releases expanded into a single
# tree in a sub-directory "ant". 
#
# NEXT TIME - remember to tag existing release first!
#
# cvs rtag "ant_v_1_5" java/source/ant
#

CVS_RSH=ssh
export CVS_RSH
cd ant
cvs  -d :ext:pwd@cvs.starlink.ac.uk:/cvs \
   import -ko -I! -m "ANT version 1.5.2 sources" java/source/ant ANT ANT_V_1_5_2

# Merged sources with command:
#
#   cvs checkout -jANT_V_1_5 -jANT_V_1_5_2 java/source/ant
#
# Then fixup any changes and re-commit.
#
